{
  "name": "@gokul/supportql",
  "version": "1.0.0",
  "description": "An MCP server to query PostgreSQL and MySQL databases using natural language",
  "main": "dist/postgres.js",
  "bin": {
    "supportql-postgres": "./dist/postgres.js",
    "supportql-mysql": "./dist/mysql.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/postgres.js",
    "./postgres": "./dist/postgres.js",
    "./mysql": "./dist/mysql.js"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gokulkrishnan3113/SupportQL.git"
  },
  "author": "Gokul Krishnan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gokulkrishnan3113/SupportQL/issues"
  },
  "homepage": "https://github.com/Gokulkrishnan3113/SupportQL#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "mysql2": "^3.14.1",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@types/pg": "^8.15.4",
    "shx": "^0.4.0",
    "typescript": "^5.8.3"
  }
}
